{
  "name": "fastify-template",
  "version": "2.1.1",
  "description": "minimal fastify template",
  "author": "mehmetsefabalik@gmail.com",
  "keywords": [
    "fastify",
    "starter",
    "boilerplate",
    "typescript",
    "mongoose",
    "mongodb",
    "jest",
    "eslint"
  ],
  "repository": "https://github.com/mehmetsefabalik/fastify-template",
  "license": "UNLICENSED",
  "scripts": {
    "build:dev": "tsc --watch",
    "build": "tsc",
    "start:dev": "NODE_ENV=development nodemon ./dist/index",
    "start": "NODE_ENV=production node ./dist/index",
    "test": "./node_modules/.bin/jest",
    "cov": "./node_modules/.bin/jest --collect-coverage",
    "lint": "./node_modules/.bin/eslint --fix ./src/*.ts ./src/**/*.ts"
  },
  "dependencies": {
    "fastify": "^3.29.5",
    "fastify-env": "^2.2.0",
    "fastify-static": "^3.4.0",
    "glob": "^7.2.3",
    "mongoose": "^6.12.6",
    "reflect-metadata": "^0.1.14",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12",
    "@types/glob": "^7.2.0",
    "@types/jest": "^26.0.24",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.18.63",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.8.8",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
